<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Food">

    <TextView
        android:id="@+id/detailphoto"
        android:layout_width="189dp"
        android:layout_height="177dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/selfphoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addphoto"
        android:layout_width="33dp"
        android:layout_height="33dp"
        android:layout_marginTop="72dp"
        android:background="@drawable/selfcamera"
        app:layout_constraintEnd_toEndOf="@+id/detailphoto"
        app:layout_constraintStart_toStartOf="@+id/detailphoto"
        app:layout_constraintTop_toTopOf="@+id/detailphoto" />

    <TextView
        android:id="@+id/bluebar"
        android:layout_width="458dp"
        android:layout_height="64dp"
        android:background="@color/darkblue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />
    <Button
        android:id="@+id/table"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="104dp"
        android:layout_marginLeft="104dp"
        android:onClick="onClick"
        android:textAllCaps="false"
        android:background="@drawable/bargraph"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        tools:ignore="OnClick"/>

    <Button
        android:id="@+id/chat"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="280dp"
        android:layout_marginLeft="280dp"
        android:background="@drawable/chat"
        android:onClick="onClick"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        tools:ignore="OnClick"/>

    <Button
        android:id="@+id/recommend"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="356dp"
        android:layout_marginLeft="356dp"
        android:onClick="onClick"
        android:textAllCaps="false"
        android:background="@drawable/yellowdelicious"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        tools:ignore="OnClick"/>
    <Button
        android:id="@+id/button"
        android:layout_width="87dp"
        android:layout_height="86dp"
        android:layout_marginBottom="16dp"
        android:onClick="onClick"
        android:textAllCaps="false"
        android:background="@drawable/ellipse3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="OnClick"/>

    <Button
        android:id="@+id/profile"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:background="@drawable/whiteuser"
        android:onClick="onClick"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.977"
        tools:ignore="OnClick"/>

    <Button
        android:id="@+id/pencil"
        android:layout_width="43dp"
        android:layout_height="49dp"
        android:layout_marginBottom="36dp"
        android:onClick="onClick"
        android:textAllCaps="false"
        android:background="@drawable/pencil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="OnClick"
        />

    <TextView
        android:id="@+id/hint"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="請拍攝或選擇一張個人臉部照片"
        android:textColor="@color/darkblue"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/detailphoto"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/selfclick"
        android:layout_width="119dp"
        android:layout_height="33dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/loginbar"
        android:text="開始分析"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detailphoto" />

    <TextView
        android:id="@+id/selfdetail"
        android:layout_width="378dp"
        android:layout_height="392dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/selfdetail"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detailphoto" />

    <TextView
        android:id="@+id/resulttitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:textSize="25dp"
        android:textColor="@color/darkblue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/selfdetail" />

    <TextView
        android:id="@+id/selfinside"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:textColor="@color/darkblue"
        android:textSize="18dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resulttitle" />

    <ImageView
        android:id="@+id/chicken"
        android:layout_width="322dp"
        android:layout_height="195dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/pencil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selfclick"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/foodname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="132dp"
        android:textColor="@color/darkblue"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/selfdetail"
        app:layout_constraintHorizontal_bias="0.127"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/selfclick" />

</androidx.constraintlayout.widget.ConstraintLayout>