<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment3">

    <!-- TODO: Update blank fragment layout -->


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="411dp"
        android:layout_height="562dp"
        android:layout_marginLeft="-50dp"
        android:layout_marginTop="-10px"
        android:layout_marginBottom="1dp"
        app:srcCompat="@drawable/background" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="411dp"
        android:layout_height="562dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="-10px"
        android:layout_marginBottom="1dp"
        app:srcCompat="@drawable/background" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="52dp"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <com.github.mikephil.charting.charts.PieChart
            android:id="@+id/pieChart"
            android:layout_width="282dp"
            android:layout_height="224dp"
            android:layout_marginLeft="90px"
            android:layout_marginTop="0px">

        </com.github.mikephil.charting.charts.PieChart>


    </LinearLayout>

    <ImageView
        android:id="@+id/fat_analysis"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="460dp"
        android:src="@drawable/fat_analysis" />

    <ImageView
        android:id="@+id/portein_analysis"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="380dp"
        android:src="@drawable/portein_analysis" />

    <ImageView
        android:id="@+id/carbon_analysis"
        android:layout_width="300dp"
        android:layout_height="65dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/carbon_analysis" />

    <ImageView
        android:id="@+id/carbonanalysis"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="305dp"
        android:layout_weight="1"
        android:src="@drawable/carbonchinese1"

        />

    <ImageView
        android:id="@+id/portineanalysis"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="385dp"
        android:layout_weight="1"
        android:src="@drawable/porteinchinese"
        android:textColor="@color/white" />

    <ImageView
        android:id="@+id/fatanalysis"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="465.5dp"
        android:layout_weight="1"
        android:src="@drawable/fatchinese"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/carbon_number"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_marginLeft="255dp"
        android:layout_marginTop="315dp"
        android:text="300 g"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/portine_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="255dp"
        android:layout_marginTop="395.5dp"
        android:text="  35 g"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/fat_number"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginTop="475dp"
        android:layout_marginLeft="255dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="  40 g" />


</FrameLayout>